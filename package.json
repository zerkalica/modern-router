{
  "name": "modern-router",
  "version": "1.1.7",
  "description": "Modern observable-based router",
  "config": {
    "builddir": "."
  },
  "scripts": {
    "postinstall": "test -d conf && conf/resources/postinstall.sh $npm_package_name || exit 0",
    "prepush": "exit 0 && npm test",
    "prepublish": "conf/resources/prepublish.sh",
    "preversion": "conf/resources/checkgit.sh && npm test && npm run build",
    "postversion": "git push && git push --tags",
    "precommit": "exit 0",
    "examples": "babel-node ./examples/server.js",
    "clean": "rm -rf $npm_package_config_builddir/dist coverage",
    "build": "npm run clean && babel src --source-maps --out-dir $npm_package_config_builddir/dist",
    "build.dev": "babel src --source-maps --out-dir $npm_package_config_builddir/dist --watch",
    "lint": "eslint src",
    "check": "flow check",
    "testonly": "mocha",
    "example.browser": "webpack-dev-server --config examples/browserapp/webpack.config.es5.js --hot --history-api-fallback --inline",
    "example.node": "babel-node ./examples/server",
    "test": "npm run lint && npm run check && npm run testonly",
    "test.dev": "npm run testonly -- --growl --watch"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/modern-router.git"
  },
  "keywords": [
    "reactive",
    "router",
    "modular",
    "state"
  ],
  "files": [
    "*.map",
    "*.js",
    "*.flow",
    ".flowconfig",
    "README.md",
    "LICENSE",
    "src",
    "dist",
    "flow-typed"
  ],
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint-config-airplus": "^2.0.10",
    "find-root": "^1.0.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "sinon": "^1.17.7",
    "webpack": "^2.x.x",
    "webpack-dev-server": "^2.x.x"
  },
  "dependencies": {
    "es6-error": "^4.0.1",
    "susanin": "^1.0.3"
  }
}
