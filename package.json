  "dependencies": {
    "mol_data_all": "^1.0.3",
    "susanin": "^1.0.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.22",
    "@types/node": "^12.11.7",
    "@types/react": "^16.9.11",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "react": "*"
  }
