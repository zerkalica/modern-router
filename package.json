{
  "name": "modern-router",
  "version": "3.0.0",
  "description": "Strongly typed, framework agnostic router",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist *.lock",
    "build": "tsc --build --force",
    "watch": "tsc --build --watch",
    "test": "jest",
    "format": "prettier --write -c 'src/**.ts'"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/zerkalica/modern-router.git"
  },
  "keywords": [
    "router",
    "reactive",
    "typed",
    "agnostic",
    "typescript"
